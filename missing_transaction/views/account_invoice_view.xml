<odoo>
	<data>
		<record id="account_move_form_inherited_to_add_transaction" model="ir.ui.view">
			<field name="name">account.move.inherited.form</field>
			<field name="model">account.move</field>
			<field name="inherit_id" ref="account.view_move_form" />
			<field name="arch" type="xml">
				<field name="partner_shipping_id" position="after">
					<field name="transaction_number" />
				</field>
			</field>
		</record>
		<record id="account_move_tree_inherited_to_add_transaction" model="ir.ui.view">
			<field name="name">account.move.inherited.tree</field>
			<field name="model">account.move</field>
			<field name="inherit_id" ref="account.view_invoice_tree" />
			<field name="arch" type="xml">
				<field name="name" position="after">
					<field name="transaction_number" />
				</field>
			</field>
		</record>
		
	</data>
</odoo>